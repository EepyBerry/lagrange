{
  "name": "lagrange",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-host": "vite --host",
    "build": "run-p type-check prettify \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "prettify": "prettier . --write",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EepyBerry/lagrange.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "typescript",
    "threejs"
  ],
  "author": "EepyBerry",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@floating-ui/vue": "^1.0.6",
    "@unhead/vue": "^1.9.14",
    "dexie": "^4.0.7",
    "file-saver": "^2.0.5",
    "iconify-icon": "^2.1.0",
    "pako": "^2.1.0",
    "three": "^0.166.0",
    "three-custom-shader-material": "^5.4.0",
    "vue": "^3.4.21",
    "vue-accessible-color-picker": "^5.0.1",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.14.11",
    "@types/pako": "^2.0.3",
    "@types/three": "^0.165.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.3.3",
    "sass": "^1.75.0",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vue-tsc": "^2.0.11"
  }
}
